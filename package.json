{
  "name": "taskr-precache",
  "version": "1.0.0",
  "description": "Generate a service worker to cache resources and make them available offline.",
  "repository": "https://github.com/marcusasplund/taskr-precache",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "lib/index.js"
  ],
  "keywords": [
    "taskr",
    "taskr-plugin",
    "precache",
    "service",
    "worker",
    "offline",
    "sw",
    "cache"
  ],
  "scripts": {
    "test": "xo && tape test/*.js | tap-spec"
  },
  "author": {
    "name": "Marcus Asplund",
    "email": "rootfood@gmail.com",
    "url": "https://pap.as"
  },
  "devDependencies": {
    "@taskr/clear": "^1.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "taskr": "^1.1.0",
    "xo": "*"
  },
  "engines": {
    "node": ">= 4.6"
  },
  "xo": {
    "semicolon": false,
    "rules": {
      "generator-star-spacing": 0,
      "capitalized-comments": 0,
      "require-yield": 0
    }
  },
  "dependencies": {
    "sw-precache": "^5.2.0",
    "sw-toolbox": "^3.6.0"
  }
}
